{
  "name": "cypress",
  "version": "1.0.0",
  "description": "Tests",
  "private": true,
  "scripts": {
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mocha",
    "pretest": "npm run clean:reports",
    "merge-report": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/full_report.json",
    "generate-report": "marge cypress/reports/full_report.json -f report -o cypress/reports ",
    "after:tests": "npm run merge-report && npm run generate-report",
    "test": "npx cypress run",
    "cypress": "npm run test || npm run after:tests",
    "start": "echo",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json",
    "tsc": "tsc --pretty --noEmit"
  },
  "author": "Irfan Charania",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@types/node": "^13.7.1",
    "cypress": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^4.0.0",
    "mochawesome-report-generator": "^4.1.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6"
  }
}